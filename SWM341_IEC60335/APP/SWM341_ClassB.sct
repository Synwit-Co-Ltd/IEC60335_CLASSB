	; section name starts with ".bss" and place the section in a region with UNINIT attribute
    CLASSB 0x20000040 UNINIT {		        ; Class B variables CLASSB变量
    SWM341xx_SelfTest_startup.o (.bss.CLASS_B_RAM)
    }

